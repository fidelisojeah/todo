{
    "name": "todo-backend-task",
    "version": "1.0.0",
    "description": "backend todo task",
    "main": "index.js",
    "scripts": {
        "clean": "rm -rf ./build/*",
        "build:ts": "tsc",
        "build": "npm run clean && npm run build:types && npm run build:js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:js": "babel src --out-dir build --extensions \".ts,.js\" --source-maps inline --copy-files",
        "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
        "test": "jest --runInBand --verbose --coverage --detectOpenHandles",
        "start": "node ./build/index.js",
        "start:dev": "nodemon --inspect=9000",
        "type-check": "tsc --noEmit",
        "type-check:watch": "npm run type-check -- --watch"
    },
    "repository": {
        "type": "git",
        "url": "git+git@github.com:fidelisojeah/todo.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/fidelisojeah/todo/issues"
    },
    "homepage": "https://github.com/fidelisojeah/todo#readme",
    "dependencies": {
        "api-query-params": "^4.15.0",
        "compression": "^1.7.4",
        "dotenv": "^8.2.0",
        "dotenv-expand": "^5.1.0",
        "express": "^4.17.1",
        "express-async-errors": "^3.1.1",
        "express-winston": "^4.0.5",
        "helmet": "^3.23.3",
        "http-status-codes": "^1.4.0",
        "moment-timezone": "^0.5.31",
        "mongoose": "^5.10.2",
        "mongoose-query-parser": "^1.1.7",
        "mongoose-unique-validator": "^2.0.3",
        "morgan": "^1.10.0",
        "swagger-ui-express": "^4.1.4",
        "uuid": "^8.3.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.11.5",
        "@babel/core": "^7.11.5",
        "@babel/node": "^7.10.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-typescript": "^7.10.4",
        "@babel/register": "^7.11.5",
        "@types/compression": "^1.7.0",
        "@types/eslint": "^6.8.1",
        "@types/express": "^4.17.8",
        "@types/faker": "^4.1.12",
        "@types/helmet": "0.0.46",
        "@types/jest": "^25.2.3",
        "@types/mongoose": "^5.7.36",
        "@types/mongoose-unique-validator": "^1.0.4",
        "@types/morgan": "^1.9.1",
        "@types/node": "^13.13.15",
        "@types/supertest": "^2.0.10",
        "@types/swagger-ui-express": "^4.1.2",
        "@types/validator": "^13.1.0",
        "@typescript-eslint/eslint-plugin": "^2.34.0",
        "@typescript-eslint/parser": "^2.34.0",
        "babel-jest": "^26.2.2",
        "babel-plugin-module-resolver": "^4.0.0",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-runtime": "^6.26.0",
        "chai": "^4.2.0",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
